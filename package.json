{
  "name": "docsify-select",
  "version": "1.0.0",
  "description": "A docsify.js plugin for variably rendering content with html select menus from markdown",
  "author": "James Hegedus",
  "license": "MIT",
  "homepage": "https://github.com/jthegedus/docsify-select#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jthegedus/docsify-select.git"
  },
  "bugs": {
    "url": "https://github.com/jthegedus/docsify-select/issues"
  },
  "keywords": [
    "docsify",
    "docsify.js",
    "docsify-plugin",
    "documentation"
  ],
  "main": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "autoprefixer": "^9.7.6",
    "browser-sync": "^2.26.7",
    "compression": "^1.7.4",
    "docsify-cli": "^4.4.0",
    "husky": "^4.2.5",
    "lodash.merge": "^4.6.2",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-custom-properties": "^9.1.1",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-import": "^12.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-postcss": "^2.5.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-uglify": "^6.0.4",
    "xo": "^0.26.1"
  },
  "scripts": {
    "build": "rollup -c",
    "serve": "node server.js",
    "start": "run-p watch serve",
    "watch": "run-p 'build -- -w'",
    "clean": "rimraf dist/",
    "fix": "xo --fix",
    "test": "xo"
  },
  "husky": {
    "hooks": {
      "pre-commit": "xo --fix",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ]
    ]
  },
  "browserslist": [
    "ie >= 10"
  ],
  "xo": {
    "env": [
      "browser"
    ],
    "ignores": [
      "src/style.scss"
    ]
  }
}
